all: nema_font_convert

CC     = gcc
CFLAGS = -std=c99 -g -Wall -I.\Freetype2\include -I.\Freetype2\include\freetype2 -L.\Freetype2\lib
LIBS   = -lm -lfreetype

nema_font_convert: main.c font_convert.c
	$(CC) $(CFLAGS) main.c font_convert.c $(LIBS) -o $@
#	strip $@

clean:
	rm nema_font_convert.exe
